package com.revature.core.java.assignment;

public class Fibonacci 
{
	public void displayFibonacciSequence(int n)
	{
		int x = 0;
		int y = 1;
		for(int i = 0; i < n; i++)
		{
			if(i <= 1)
			{
				System.out.println(i);
			}
			else
			{
				int temp = x + y;
				System.out.println(temp);
				x = y;
				y = temp;
			}
		}
	}

}
