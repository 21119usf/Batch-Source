package homework1;

public class Q2 {
	//Write a program to display the first 25 Fibonacci numbers beginning at 0.
	public static void main(String[] args) {
		int prev = 0;
		int curr = 1;
		int next;
		System.out.print(prev + " " + curr);
		for (int count = 1; count < 25; count++) {
			next = prev + curr;
			prev = curr;
			curr = next;
			System.out.print(" " + curr);
		}
	}
}
