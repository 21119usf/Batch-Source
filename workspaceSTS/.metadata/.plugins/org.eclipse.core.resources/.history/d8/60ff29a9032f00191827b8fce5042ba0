package com.revature.example;

public class LoopExample {
/*
 * doWhile
 * while
 * for
 * if
 * switch
 */
	
	static int[] myArray = {1,0,5,6,3,2,3,7,9,8}; //10 elements
	
	public static void main(String[] args) {
		int counter = 0;
		do {
			System.out.println(myArray[counter--]);
		} while (counter>0);
		
		for(int i = 0; i < 10; i++) {
			System.out.print(myArray[i] + " ");
		}
	}
}
