package com.revature.generics;

public class GenDemo {

	public static void main(String[] args) {
		//Create a Gen1 Reference for Integers
		Gen1<Integer> i;
		i = new Gen1<Integer>(88);
		
		//show type of i
		i.showType();
		
		//get value of i
		int v = i.getObj();
		
		System.out.println("value: " + v);
	}
}
