package driver;

import java.util.Scanner;

import beans.*;

public class Driver {
	public static Bank bank = new Bank();
	
	public static void main(String[] args) {
		int option;
		
		option = bank.start();
		
		switch (option) {
		case 1:
			bank.login();
			break;
		case 2:
			bank.registerApplication();
		}
	}
}
