package com.revature.junit5;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class StringTest {

	@Test
	void toUpperCase() {
		String str = "abcd";
		String result = str.toUpperCase();
		//assertEquals(<Expected>, <Actual>)
		assertEquals("ABCD", result);
	}

}
